Title=<%= @run.name %>
Attempts=<%= @run.respond_to?(:attempts) ? @run.attempts : 0 %>
Offset=<%= @run.respond_to?(:offset) ? @run.offset : 0 %>
Size=<%= @run.respond_to?(:size) ? @run.size : "152,25" %>
<% @run.splits.each do |split| %>
<%= split.name %>,0,<%= split.finish_time %>,<%= split.best || split.duration %>
<% end %>
Icons=<%= ('"",' * @run.splits.length)[0..-2].html_safe %>
