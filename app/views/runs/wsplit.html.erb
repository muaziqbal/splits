Title=<%= @run.to_s %>
Attempts=<%= @run.respond_to?(:attempts) ? @run.attempts : 0 %>
Offset=<%= @run.respond_to?(:offset) ? @run.offset : 0 %>
Size=<%= @run.respond_to?(:size) ? @run.size : "152,25" %>
<% @run.segments.each do |segment| %>
<%= segment.name %>,0,<%= segment.finish_time %>,<%= segment.best || segment.duration %>
<% end %>
Icons=<%= ('"",' * @run.segments.length)[0..-2].html_safe %>
